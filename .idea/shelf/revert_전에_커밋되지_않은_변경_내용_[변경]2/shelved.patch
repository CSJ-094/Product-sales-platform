+        SELECT PROD_ID, CAT_ID, IS_MAIN, PC_REG
+        FROM PRODUCT_CATEGORY_DB
+        WHERE PROD_ID = #{prodId}
+    </select>
+
+    <!-- 특정 상품의 매핑 전체 삭제 -->
+    <delete id="deleteAllByProdId" parameterType="long">
